# === Snowflake connection (fill these and rename to .env) ===
SNOWFLAKE_ACCOUNT=your_account_name   # e.g., ab12345.eu-central-1
SNOWFLAKE_USER=your_user
SNOWFLAKE_PASSWORD=your_password      # or use key pair auth; see README
SNOWFLAKE_ROLE=SYSADMIN
SNOWFLAKE_WAREHOUSE=COMPUTE_WH
SNOWFLAKE_DATABASE=IOT
SNOWFLAKE_SCHEMA=PUBLIC
# Optional table names (match the SQL setup script)
SNOWFLAKE_STAGING_TABLE=RTLS_STAGING
SNOWFLAKE_POSITIONS_VIEW=RTLS_POSITIONS_V
SNOWFLAKE_KPIS_TABLE=RTLS_KPIS_DAILY
